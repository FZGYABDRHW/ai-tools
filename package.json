{
  "name": "wowworks-ai-tools",
  "productName": "Wowworks AI Tools",
  "version": "1.0.6",
  "description": "Wowworks AI Tools - A modern Electron application for AI-powered task building and report generation",
  "homepage": "https://wowworks.ru",
  "author": "vn-ww",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:mac": "electron-forge make --platform darwin",
    "make:dmg": "electron-forge make --platform darwin --targets @electron-forge/maker-dmg",
    "publish": "electron-forge publish",
    "publish:github": "electron-forge publish --target=@electron-forge/publisher-github",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-dmg": "^7.8.3",
