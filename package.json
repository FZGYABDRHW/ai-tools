    "node-loader": "^2.0.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@toast-ui/react-editor": "^3.2.3",
    "antd": "^5.27.1",
    "axios": "0.18.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
